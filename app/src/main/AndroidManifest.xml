<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RateMyProf"
        tools:targetApi="31">
        <activity
            android:name=".StudentHomePage"
            android:exported="true"
            android:label="@string/title_activity_student_home_page"
            android:theme="@style/Theme.RateMyProf" />
        <activity
            android:name=".FormSubmittedActivity"
            android:exported="false"
            android:label="@string/title_activity_form_submitted"
            android:theme="@style/Theme.RateMyProf" />
        <activity
            android:name=".AdminLogin"
            android:exported="false"
            android:label="AdminLogin"
            android:theme="@style/Theme.RateMyProf" />
        <activity
            android:name=".StudentLogin"
            android:exported="false"
            android:label="StudentLogin"
            android:theme="@style/Theme.RateMyProf" />
        <activity
            android:name=".ProfessorLogin"
            android:exported="false"
            android:label="ProfessorLogin"
            android:theme="@style/Theme.RateMyProf" />
        <activity
            android:name=".FeedbackActivity"
            android:exported="false"
            android:label="Feedback"
            android:theme="@style/Theme.RateMyProf" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.RateMyProf">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>